{{!-- Training --}}
{{#if settings.show_training}}
  <section class="section training">
    <div class="row training__container">

      {{#if settings.show_training_link_1}}
        <div class="col-12 col-md-4">
          <a id="training-1" href="{{settings.training_1_url}}" class="training__item" target="{{settings.training_1_target}}" data-plugin-internal-url>
            <span class="training__icon">
              <img src="{{settings.trainings_1_icon}}" alt="{{settings.training_1_title}}" data-plugin-inline-svg>
            </span>
            <span class="h3 training__title">
              {{#is settings.training_1_title '-'}}
                FAQ
              {{else}}
                {{#if settings.translations_enable}}
                  {{dc settings.training_1_title}}
                {{else}}
                  {{settings.training_1_title}}
                {{/if}}
              {{/is}}
            </span>
            <span class="training__description">
              {{#is settings.training_1_description '-'}}
                {{#is help_center.locale 'en-us'}}Questions? Find the answer to any question.{{/is}}
              {{else}}
                {{#if settings.translations_enable}}
                  {{dc settings.training_1_description}}
                {{else}}
                  {{settings.training_1_description}}
                {{/if}}
              {{/is}}
            </span>
          </a>
        </div>
      {{/if}}
      {{#if settings.show_training_2}}
        <div class="col-12 col-md-4">
          <a id="training-2" href="{{settings.training_2_url}}" class="training__item" target="{{settings.training_2_target}}" data-plugin-internal-url>
            <span class="training__icon">
              <img src="{{settings.training_2_icon}}" alt="{{settings.training_2_title}}" data-plugin-inline-svg>
            </span>
