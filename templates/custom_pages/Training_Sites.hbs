              {{#is settings.training_2_title '-'}}
                {{#is help_center.locale 'en-us'}}Guides{{/is}}
              {{else}}
                {{#if settings.translations_enable}}
                  {{dc settings.training_2_title}}
                {{else}}
                  {{settings.training_2_title}}
                {{/if}}
              {{/is}}
            </span>
            <span class="training__description">
              {{#is settings.training_2_description '-'}}
                {{#is help_center.locale 'en-us'}}Need help? We have you covered.{{/is}}
              {{else}}
                {{#if settings.translations_enable}}
                  {{dc settings.training_2_description}}
                {{else}}
                  {{settings.training_2_description}}
                {{/if}}
              {{/is}}
            </span>
          </a>
        </div>
      {{/if}}
      {{#if settings.show_training_3}}
