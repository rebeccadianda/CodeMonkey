<meta content="width=device-width, initial-scale=1.0" name="viewport"/>
<script !src="">
  window.theme = {};
  window.theme.locale = {{json_stringify help_center.locale}};
  window.theme.darkMode = {{settings.dark_mode}};
  window.theme.prefersColorScheme = {{settings.prefers_color_scheme}};

  document.documentElement.classList.add(window.theme.darkMode ? 'ui-dark' : 'ui-light');

  if (window.MSInputMethodContext && document.documentMode) {
    document.write(
        '<script src="https://cdn.jsdelivr.net/gh/nuxodin/ie11CustomProperties@4.1.0/ie11CustomProperties.min.js"><\x2fscript>');
  }
</script>

{{#if settings.inject_head}}
  <script>
    document.write(({{json_stringify settings.inject_head}}).replace(/(&lt;)/g, '<').replace(/(&gt;)/g, '>'));
  </script>
{{/if}}

